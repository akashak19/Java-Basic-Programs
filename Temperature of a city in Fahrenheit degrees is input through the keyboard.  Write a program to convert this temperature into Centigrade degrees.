import java.util.Scanner;
class ConvertTemperature
 {

	public static void main(String[] args) {
		

		double f,c;
		Scanner sc = new Scanner(System.in);
		System.out.println("Temperature of a city in Fahrenheit degrees");
		f=sc.nextDouble();

		c=(f-32)*5/9;

		System.out.println("Temperature into Centigrade degrees = "+c);
		

}

}
